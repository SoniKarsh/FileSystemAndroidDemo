<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
		  package="com.sriyanksiddhartha.filesystemdemo">

	<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity android:name="com.karshsoni.filesystemdemo.displayscreens.MainActivity">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>

				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>

		<activity android:name="com.karshsoni.filesystemdemo.displayscreens.storageoptions.InternalStorageDemo"/>
		<activity android:name="com.karshsoni.filesystemdemo.displayscreens.InternalStorageDisplay"/>

		<activity android:name="com.karshsoni.filesystemdemo.displayscreens.storageoptions.CacheStorageDemo"/>
		<activity android:name="com.karshsoni.filesystemdemo.displayscreens.storageoptions.ExternalStorageDemo"/>

	</application>

</manifest>